package opComparacao;

public class OperadoresdeComparacao {
	
	public static void main(String[] args) {
		double numeroXis = 3;
		double numeroYplo = 2;
		boolean qualquer;
		
		qualquer = numeroXis > numeroYplo; //MaiorQue
		System.out.println("É:" +qualquer+" \n");
		
		qualquer = numeroXis < numeroYplo; //MenorQue
		System.out.println("É:" +qualquer+" \n");
		
		qualquer = numeroXis == numeroYplo; //IgualA
		System.out.println("É: "+qualquer + "\n");
		
		qualquer = numeroXis >= numeroYplo; //MaiorQue our IgualA
		System.out.println("É: "+qualquer+"\n");
		
		qualquer = numeroXis <= numeroYplo; //MenorQue our IgualA
		System.out.println("É: "+qualquer+"\n");
		
		qualquer = numeroXis != numeroYplo; //DiferenteDe
		System.out.println("É: "+qualquer+"\n");
		
	}

}